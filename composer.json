{
    "name": "diego182/cakephp-adminlte",
    "description": "Admin Panel made with AdminLTE as a plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.0",
        "cakephp/cakephp": ">=3.5.0 <4.0.0",
        "friendsofcake/bootstrap-ui": "^1.4"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "CakePHPAdminLTE\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePHPAdminLTE\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
